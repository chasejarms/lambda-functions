{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES2020",
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "outDir": "./built",
        "sourceMap": false,
        "moduleResolution": "node",
        "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "**/*.spec.ts"]
}
